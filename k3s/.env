# K3s
K3S_VERSION=v1.28.7+k3s1
K3S_KUBECONFIG_PATH=/etc/rancher/k3s/k3s.yaml

# K3s Server
K3S_SERVER_CIDR=172.16.0.0/16 # up to 172.31.0.0/16

# K3s Agent
K3S_AGENT_URL=https://myserver:6443 
K3S_AGENT_TOKEN=mynodetoken

# Rancher
RANCHER_HOSTNAME=example.com
RANCHER_VERSION=2.8.2
RANCHER_BOOTSTRAP_PASSWORD=supersecretpassword

# Cert Manager
CERT_MANAGER_VERSION=v1.12.7
LETSENCRYPT_EMAIL=sample@gmail.com
